{
  "name": "sinkar",
  "version": "0.2.5",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "tsc -p . && copyfiles -u 2 \"src/renderer/**/*.html\" dist/renderer && copyfiles -f src/icon.* dist",
    "dev": "tsc -w"
  },
  "dependencies": {
    "electron": "^25.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polycaman/sinkar.git"
  },
  "keywords": [
    "electron",
    "git",
    "application"
  ],
